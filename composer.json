{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-pcntl": "^7.2",
        "ext-posix": "^7.2",
        "bugsnag/bugsnag-laravel": "^2.21",
        "doctrine/dbal": "^2.12",
        "emotality/nova-theme": "*",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "geoip2/geoip2": "^2.11",
        "guzzlehttp/guzzle": "^7.2.0",
        "intervention/image": "^2.5",
        "laravel/framework": "^8.26",
        "laravel/horizon": "^5.6",
        "laravel/nova": "^3.21",
        "laravel/tinker": "^2.6",
        "laravelista/ekko": "^3.2",
        "predis/predis": "^1.1",
        "spatie/laravel-honeypot": "^3.0",
        "torann/geoip": "^3.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.5",
        "barryvdh/laravel-ide-helper": "^2.9",
        "beyondcode/laravel-dump-server": "^1.7",
        "deployer/recipes": "^6.2",
        "facade/ignition": "^2.5.9",
        "fzaninotto/faker": "^1.9.2",
        "laravel/ui": "^3.2",
        "lorisleiva/laravel-deployer": "^0.3.5",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.3",
        "phpunit/phpunit": "^9.3"
    },
    "repositories": [
        {
            "type": "path",
            "url": "./repositories/nova"
        },
        {
            "type": "path",
            "url": "./repositories/nova-components/NovaTheme"
        }
    ],
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "files": [
            "app/Helpers/helper.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan horizon:publish --quiet",
            "@php artisan nova:publish --quiet",
            "@php artisan optimize:clear --quiet",
            "@php artisan logs:clear --quiet",
            "@php artisan ide --quiet"
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
